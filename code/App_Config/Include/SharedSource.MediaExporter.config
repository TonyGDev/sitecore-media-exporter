<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:x="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <commands>
      <command name="mediaexporter:export" type="SharedSource.MediaExporterModule.MediaExportCommand, SharedSource.MediaExporterModule" />
    </commands>
    <settings>
      
      <!-- The name of the directory under the Sitecore installation 
      Data folder where exported zips will be stored -->
      <setting name="SharedSource.MediaExporterModule.ExportFolderName" value="MediaExports" />
      
    </settings>
  </sitecore>
</configuration>